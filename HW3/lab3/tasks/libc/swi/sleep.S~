/** @file sleep.S
 *
 * @brief sleep sycall wrapper
 *
 * @author Kartik Subramanian <ksubrama@andrew.cmu.edu>
 * Group -
	Varun Kohli <vkohli@andrew.cmu.edu>
	Carlos Gil <cgil@andrew.cmu.edu>
	Rishabh Alaap Singh <rasingh@andrew.cmu.edu>
 * @date 2008-10-31
 */

#include <asm.h>

	.file "sleep.S"

FUNC(sleep)
	stmfd sp!, {r1-r12, lr}
	swi SLEEP_SWI
	ldmfd sp!, {r1-r12, lr}
	mov pc, lr
